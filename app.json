{
  "expo": {
    "name": "app-Emhur",
    "slug": "app-Emhur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "appemhur",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash01.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabrielsouza.appEmhur"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d681dc5a-5d43-4e64-a48a-04745fd3335a"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo acessa suas fotos para que você possa compartilhá-las."
        }
      ]
    ]
  }
}
